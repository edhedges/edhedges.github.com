---
layout: nil
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>Eddie Hedges</title>
  <link href="http://edhedges.github.com" />
  <link href="http://edhedges.github.com/atom.xml" rel="self" type="application/atom+xml" />
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://edhedges.github.com</id>
  <author>
    <name>Eddie Hedges</name>
    <email>eddiehedges@gmail.com</email>
  </author>
  {% for post in site.posts %}
  <entry>
    <title>{{ post.title }}</title>
    <link href="http://edhedges.github.com{{ post.url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>http://edhedges.github.com{{ post.id }}</id>
    <author>
      <name>Eddie Hedges</name>
      <uri>http://edhedges.github.com</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
</feed>